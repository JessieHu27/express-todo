{
  "name": "express-todo",
  "version": "1.0.0",
  "description": "learn nodejs by express framework",
  "main": "index.html",
  "scripts": {
    "app": "node ./node/server.js",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessiehu27/express-todo.git"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "git-cz": "^4.3.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "validate-commit-msg": "^2.14.0"
  },
  "keywords": [
    "nodejs",
    "express",
    "demo"
  ],
  "author": "jessiehu27",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jessiehu27/express-todo/issues"
  },
  "homepage": "https://github.com/jessiehu27/express-todo#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "husky": {
      "hooks": {
        "pre-commit": "npm run lint",
        "commit-msg": "validate-commit-msg"
      }
    }
  }
}
